/*
	Telepített összetevők (Arch Linux csomagnevek):
		terminál emulátor: terminator
		font: terminus-font
		icon:  arc-icon-theme
*/
configuration {
	modi: "window,drun,run,ssh";
	width: 25;
	lines: 15;
	columns: 1;
	font: "Terminus Bold 16";
	show-icons: true;
	icon-theme: "Arc";
	terminal: "terminator";
	disable-history: true;
	sort: true;
	sorting-method: "normal";
	case-sensitive: false;
	parse-hosts: false;
	parse-known-hosts: false;
}

@theme "fancy"
